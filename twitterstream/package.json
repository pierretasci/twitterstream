{
  "name": "twitterstream",
  "version": "0.0.0",
  "private": false,
  "scripts": {
    "watch": "watchify react/app.jsx -o public/js/bundle.js -v",
    "browserify": "browserify react/app.jsx | uglifyjs > public/js/bundle.js",
    "build": "npm run browserify ",
    "start": "npm run watch & nodemon ./bin/www",
    "deploy": "node ./bin/www"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0",
    "node-jsx": "^0.13.3",
    "browserify": "^11.0.0",
    "moment": "^2.10.3",
    "react": "^0.13.3",
    "react-bootstrap": "^0.24.0",
    "reactify": "^1.1.1",
    "twitter": "^1.2.5",
    "uglify-js": "^2.4.24",
    "watchify": "^3.3.0"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  }
}
